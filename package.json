{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "5.4.0"
  },
  "scripts": {
    "new": "hexo new ${title}",
    "s": "hexo s",
    "g": "hexo g",
    "d": "hexo g && hexo d",
    "cz": "git add . && git cz",
    "log": "conventional-changelog --config node_modules/jimi-web-changelog/lib/log -i CHANGELOG.md -s -r 0",
    "v": "standard-version -r patch"
  },
  "dependencies": {
    "hexo": "^5.4.0",
    "hexo-baidu-url-submit": "^0.0.6",
    "hexo-deployer-git": "^3.0.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-baidu-sitemap": "^0.1.2",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-search": "^2.4.3",
    "hexo-generator-sitemap": "^2.1.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-helper-post-top": "^0.0.2",
    "hexo-helper-qrcode": "^1.0.2",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^4.0.0",
    "hexo-renderer-stylus": "^2.0.1",
    "hexo-server": "^2.0.0",
    "hexo-tag-aplayer": "^3.0.4",
    "hexo-tag-dplayer": "^0.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/jimi-web-changelog/lib/cz"
    }
  },
  "repository": "ssh://git@github.com/nicecoders/nicecoders.github.io.git",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.0.11",
    "jimi-web-changelog": "^0.1.7"
  }
}
